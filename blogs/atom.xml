---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Testdoc's Bolg</title>
	<link href="http://testdoc.github.io/blogs/atom.xml" rel="self"/>
	<link href="http://testdoc.github.io/blogs"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://testdoc.github.io/blogs</id>
	<author>
		<name>Gerti Poppel</name>
		<email>gerti.poppel@freenet.de</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://testdoc.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://testdoc.github.io/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
